<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfaz.Interfaz">
   <children>
      <MenuBar prefHeight="25.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
            <Menu mnemonicParsing="false" text="Perfil">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#logout" text="Logout" />
              </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Pacientes">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#loadSearch" text="Buscar" />
                  <MenuItem mnemonicParsing="false" onAction="#agregarPaciente" text="Agregar" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <AnchorPane fx:id="scene_panel" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0" />
   </children>
</AnchorPane>
